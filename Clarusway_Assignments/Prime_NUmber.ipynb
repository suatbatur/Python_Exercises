      "source": [
        "#prime number\n",
        "\n",
        "number = int(input(\"enter a number : \"))\n",
        "counter= 0\n",
        "for i in range(1,number+1):\n",
        "  if number % i==0:\n",
        "      counter+=1\n",
        "if counter >2:\n",
        "  print(\"{} is not a prime number.\".format(number))\n",
        "else:\n",
        "  print(\"{} is a prime number.\".format(number))"
      ],
      "execution_count": null,
      "outputs": []
    }

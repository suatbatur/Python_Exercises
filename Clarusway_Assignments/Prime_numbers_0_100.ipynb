        "prime_numbers=[]\n",
        "for sayi in range(1,101):  \n",
        "   if sayi > 1:  \n",
        "       for i in range(2,sayi):  \n",
        "           if (sayi % i) == 0:  \n",
        "               break  \n",
        "       else:  \n",
        "           prime_numbers.append(sayi)\n",
        "print(prime_numbers) "
      ],
